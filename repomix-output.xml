    await mineBlock(bot, "acacia_log", 5 - acaciaLogsCount);
    bot.chat("Mined more acacia logs.");
    await craftItem(bot, "acacia_planks", 5 - acaciaPlanksCount);
    bot.chat("Crafted more acacia planks.");
